# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
max_line_length = 120
end_of_line = crlf
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 2
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
